<!DOCTYPE html><%@taglib uri="http://www.springframework.org/tags/form" prefix="form"%><%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions" %><html lang="en"><head>    <!-- Access the bootstrap Css like this,        Spring boot will handle the resource mapping automcatically -->    <link rel="stylesheet" type="text/css" href="../webjars/bootstrap/4.3.1/css/bootstrap.min.css" /></head><body><nav class="navbar navbar-expand-lg navbar-light navbar-laravel">    <div class="container">        <a class="navbar-brand">Coursework</a>        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">            <span class="navbar-toggler-icon"></span>        </button>        <div class="collapse navbar-collapse" id="navbarSupportedContent">            <ul class="navbar-nav ml-auto">                <li class="nav-item">                    <a class="nav-link" href="../manufactures">Manufactures</a>                </li>                <li class="nav-item">                    <a class="nav-link" href="../models">Models</a>                </li>                <li class="nav-item">                    <a class="nav-link" href="../cars">Cars</a>                </li>                <li class="nav-item">                    <a class="nav-link" href="../attorneyPowers">Attorney powers</a>                </li>                <li class="nav-item">                    <a class="nav-link" href="../resetPassword">Reset password</a>                </li>                <li class="nav-item">                    <a class="nav-link" href="../logout">Logout</a>                </li>            </ul>        </div>    </div></nav><main class="login-form">    <div class="cotainer">        <div class="row justify-content-center">            <div class="col-md-8">                <div class="card">                    <div class="card-header">Add attorney power</div>                    <div class="card-body">                        <form:form action="../addAttorneyPower" method="POST" modelAttribute="attorneyPower">                            <div class="form-group row">                                <label for="personId" class="col-md-4 col-form-label text-md-right">Person</label>                                <div class="col-md-6">                                    <form:select path="personId" type="text" id="personId" cssClass="form-control">                                        <form:option value="" label=""/>                                        <c:forEach items="${persons}" var="person">                                            <form:option value="${person.id}" label="${person.firstName} ${person.lastName}"/>                                        </c:forEach>                                    </form:select>                                    <form:errors path="personId" cssStyle="color: red"/>                                </div>                            </div>                            <div class="form-group row">                                <label for="carId" class="col-md-4 col-form-label text-md-right">Car</label>                                <div class="col-md-6">                                    <form:select path="carId" type="text" id="carId" cssClass="form-control">                                        <form:option value="" label=""/>                                        <c:forEach items="${cars}" var="car">                                            <form:option value="${car.id}" label="${car.model.manufacturer.name}, ${car.model.name} - ${car.number}"/>                                        </c:forEach>                                    </form:select>                                    <form:errors path="carId" cssStyle="color: red"/>                                </div>                            </div>                            <div class="form-group row">                                <label for="dateFrom" class="col-md-4 col-form-label text-md-right">Date from(YYYY/MM/DD)</label>                                <div class="col-md-6">                                    <form:input path="dateFrom" type="text" id="dateFrom" cssClass="form-control"/>                                    <form:errors path="dateFrom" cssStyle="color: red"/>                                </div>                            </div>                            <div class="form-group row">                                <label for="dateTo" class="col-md-4 col-form-label text-md-right">Date to(YYYY/MM/DD)</label>                                <div class="col-md-6">                                    <form:input path="dateTo" type="text" id="dateTo" cssClass="form-control"/>                                    <form:errors path="dateTo" cssStyle="color: red"/>                                </div>                            </div>                            <input type="hidden" name="${_csrf.parameterName}" value="${_csrf.token}"/>                            <div class="col-md-6 offset-md-4">                                <button type="submit" class="btn btn-primary">                                    Add                                </button>                            </div>                        </form:form>                    </div>                </div>            </div>        </div>    </div></main><script type="text/javascript" src="../webjars/jquery/3.4.1/jquery.min.js"></script><script type="text/javascript" src="../webjars/bootstrap/4.3.1/js/bootstrap.min.js"></script></body></html>