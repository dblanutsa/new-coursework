<!DOCTYPE html><%@taglib uri="http://www.springframework.org/tags/form" prefix="form"%><html lang="en"><head>    <!-- Access the bootstrap Css like this,        Spring boot will handle the resource mapping automcatically -->    <link rel="stylesheet" type="text/css" href="../webjars/bootstrap/4.3.1/css/bootstrap.min.css" /></head><body><nav class="navbar navbar-expand-lg navbar-light navbar-laravel">    <div class="container">        <a class="navbar-brand">Coursework</a>        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">            <span class="navbar-toggler-icon"></span>        </button>        <div class="collapse navbar-collapse" id="navbarSupportedContent">            <ul class="navbar-nav ml-auto">                <li class="nav-item">                    <a class="nav-link" href="../manufactures">Manufactures</a>                </li>                <li class="nav-item">                    <a class="nav-link" href="../models">Models</a>                </li>                <li class="nav-item">                    <a class="nav-link" href="../cars">Cars</a>                </li>                <li class="nav-item">                    <a class="nav-link" href="../attorneyPowers">Attorney powers</a>                </li>                <li class="nav-item">                    <a class="nav-link" href="../resetPassword">Reset password</a>                </li>                <li class="nav-item">                    <a class="nav-link" href="../logout">Logout</a>                </li>            </ul>        </div>    </div></nav><main class="login-form">    <div class="cotainer">        <div class="row justify-content-center">            <div class="col-md-8">                <div class="card">                    <div class="card-header">Add manufacturer</div>                    <div class="card-body">                        <form:form action="../addManufacturer" method="POST" modelAttribute="manufacturer">                            <div class="form-group row">                                <label for="name" class="col-md-4 col-form-label text-md-right">Name</label>                                <div class="col-md-6">                                    <form:input path="name" type="text" id="name" cssClass="form-control"/>                                    <form:errors path="name" cssStyle="color: red"/>                                </div>                            </div>                            <div class="form-group row">                                <label for="description" class="col-md-4 col-form-label text-md-right">Description</label>                                <div class="col-md-6">                                    <form:input path="description" type="text" id="description" cssClass="form-control"/>                                    <form:errors path="description" cssStyle="color: red"/>                                </div>                            </div>                            <input type="hidden" name="${_csrf.parameterName}" value="${_csrf.token}"/>                            <div class="col-md-6 offset-md-4">                                <button type="submit" class="btn btn-primary">                                   Add                                </button>                            </div>                        </form:form>                    </div>                </div>            </div>        </div>    </div></main><script type="text/javascript" src="../webjars/jquery/3.4.1/jquery.min.js"></script><script type="text/javascript" src="../webjars/bootstrap/4.3.1/js/bootstrap.min.js"></script></body></html>